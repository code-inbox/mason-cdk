  "stability": "experimental",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "java": {
        "package": "io.github.cdklabs.mason",
        "maven": {
          "groupId": "io.github.cdklabs",
          "artifactId": "mason"
        }
