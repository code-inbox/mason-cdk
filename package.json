      "python": {
        "distName": "mason",
        "module": "mason"
      },
      "dotnet": {
        "namespace": "CdklabsMason",
        "packageId": "CdklabsMason"
      },
      "go": {
        "moduleName": "github.com/cdklabs/mason-go"
      }
    },
    "tsc": {
      "outDir": "lib",
      "rootDir": "src"
    }
